        "report4['FICO_Conditions'] =    np.where(x < 600, '< 600', \n",
        "                                        np.where((x>=600) & (x<700), '600 - 699', \n",
        "                                                np.where((x>=700)&(x<800), '700 - 799', \n",
        "                                                        np.where(x>=800, '>= 800', 0)\n",
        "                                                        )\n",
        "                                                )\n",
        "                                      )"
      ],
      "execution_count": null,
      "outputs": []
    },
