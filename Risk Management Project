        "x = report3['LOAN_AGE']\n",
        "report3['Conditions'] = np.where(x < 10, '0 - 9 Months',\n",
        "                                np.where((x >= 10) & (x < 20), '10 - 19 Months',\n",
        "                                        np.where((x >= 20) & (x < 30), '20 - 29 Months',\n",
        "                                                np.where((x >= 30) & (x < 40), '30 - 39 Months',\n",
        "                                                        np.where(x >= 40, '>=40 Months', 'Unknown')\n",
        "                                                )         \n",
        "                                        )          \n",
        "                                )\n",
        "                        )"
      ],
